	author = {Dock-Nascimento, D. B. and Aguilar-Nascimento, J. E. and Linetzky Waitzberg, D.},
	booktitle = {Rev Col Bras Cir},
	da = {Dec},
	date-added = {2021-04-09 14:18:35 -0500},
